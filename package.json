{
	"name": "axiehelper",
	"version": "2.0.0",
	"description": "Discord Bot for Axie Infinity",
	"private": true,
	"license": "GPL-3.0",
	"main": "dist/app",
	"scripts": {
		"cleardir": "rimraf dist/*",
		"compile": "tsc",
		"build": "npm run cleardir && npm run compile",
		"deploy": "node --enable-source-maps --trace-warnings dist/deploy-commands.js",
		"deploy:dev": "cross-env NODE_ENV=development npm run deploy",
		"deploy:prod": "cross-env NODE_ENV=production npm run deploy",
		"start": "node --enable-source-maps --trace-warnings .",
		"dev": "cross-env NODE_ENV=development ts-node src/app.ts --watch",
		"prod": "cross-env NODE_ENV=production npm run start",
		"migrate:dev": "npm run prisma:migrate && npm run prisma:push && npm run dev",
		"migrate:prod": "npm run prisma:migrate && npm run prisma:push && npm run prod",
		"prisma:push": "npx prisma db push",
		"prisma:migrate": "npx prisma migrate deploy"
	},
	"engines": {
		"node": ">=18.12.x"
	},
	"dependencies": {
		"@prisma/client": "^4.8.1",
		"agp-npm": "^1.0.11",
		"axios": "^0.26.1",
		"canvas": "^2.9.1",
		"canvas-constructor": "^6.0.2",
		"chart.js": "^3.9.1",
		"chartjs-node-canvas": "^4.1.6",
		"chartjs-plugin-datalabels": "^2.2.0",
		"cron": "^1.8.2",
		"cross-env": "^7.0.3",
		"dayjs": "^1.11.1",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.0",
		"ethers": "^5.6.9",
		"fs-extra": "^11.1.0",
		"fuse.js": "^6.6.2",
		"graphql-request": "^5.2.0",
		"i18next": "^21.8.14",
		"i18next-fs-backend": "^1.1.4",
		"ioredis": "^5.0.4",
		"module-alias": "^2.2.2",
		"p-throttle": "4.1.1",
		"pino": "^8.3.0"
	},
	"overrides": {
		"chart.js": "3.9.1",
		"p-throttle": "4.1.1"
	},
	"devDependencies": {
		"@types/cron": "^2.0.1",
		"@types/fs-extra": "^9.0.13",
		"@types/i18next-fs-backend": "^1.1.2",
		"@types/ioredis": "^4.28.10",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^18.6.2",
		"concurrently": "^7.6.0",
		"pino-pretty": "^8.1.0",
		"prisma": "^4.12.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	},
	"_moduleAliases": {
		"@apis": "dist/services/apis",
		"@assets": "assets",
		"@client": "dist/client",
		"@configs": "dist/configs",
		"@constants": "dist/constants",
		"@custom-types": "dist/types",
		"@jobs": "dist/jobs",
		"@utils": "dist/utils",
		"@models": "dist/models",
		"@services": "dist/services",
		"pino-logger": "dist/logger.js"
	}
}
